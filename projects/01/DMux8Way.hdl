/**
 * 8-way demultiplexor:
 * {a, b, c, d, e, f, g, h} = {in, 0, 0, 0, 0, 0, 0, 0} if sel == 000
 *                            {0, in, 0, 0, 0, 0, 0, 0} if sel == 001
 *                            etc.
 *                            {0, 0, 0, 0, 0, 0, 0, in} if sel == 111
 */

CHIP DMux8Way {
    IN in, sel[3];
    OUT a, b, c, d, e, f, g, h;

    PARTS:
    // Put your code here:
    DMux4Way(in=in, sel=sel[1..2], a=res0, b=res1, c=res2, d=res3);
    DMux(in=res0, sel=sel[0], a=a, b=b);
    DMux(in=res1, sel=sel[0], a=c, b=d);
    DMux(in=res2, sel=sel[0], a=e, b=f);
    DMux(in=res3, sel=sel[0], a=g, b=h);
}